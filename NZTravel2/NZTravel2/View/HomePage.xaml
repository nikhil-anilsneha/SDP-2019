<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NZTravel2.View.HomePage">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="MapButton" Text="MAP" Clicked="MapButtonClicked"/>
            <Button x:Name="ItineraryButton" Text="ITINERARY" Clicked="ItineraryButton_Clicked"/>
            <Button x:Name="AttractionButton" Text="ATTRACTIONS" Clicked="AttractionButton_Clicked"/>
            <Button x:Name="FuelButton" Text="FUEL" Clicked="FuelButton_Clicked"/>
            <Button x:Name="RestaurantButton" Text="RESTAURANT" Clicked="RestaurantButton_Clicked"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>